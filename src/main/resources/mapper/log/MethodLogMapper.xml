<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.development.module.log.mapper.MethodLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.spring.development.module.log.entity.MethodLog">
        <id column="id" property="id" />
        <result column="remote_ip" property="remoteIp" />
        <result column="local_ip" property="localIp" />
        <result column="method_name" property="methodName" />
        <result column="invoke_time" property="invokeTime" />
        <result column="input" property="input" />
        <result column="output" property="output" />
        <result column="consume_time" property="consumeTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, remote_ip, local_ip, method_name, invoke_time, input, output, consume_time
    </sql>

</mapper>
